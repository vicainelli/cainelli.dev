---
export interface Props {
  title: string;
  eventName: string;
  href: string;
}

const { href, title, eventName } = Astro.props;
---

<a
  class="font-sans dark:text-white lowercase font-thin mx-4"
  href={href}
  rel="noopener"
  target="_blank"
  onclick="ga('send', 'event', 'button', 'click', {eventName});">{title}</a
>